{
    "name": "crewlabs/oauth2-dribbble",
    "description": "Dribbble OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on",
        "Dribbble",
        "Dribble"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Baker",
            "email": "dave@crew.co"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "CrewLabs\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CrewLabs\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}
